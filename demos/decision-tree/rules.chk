  @module goal
  state start
  outlook cloudy
}
golf a6 {
  @module goal
  state done
}
golf a7 {
  @module output
  value "play"
}

rule r5 {
  @condition g5
  @action a8
}
golf g5 {
  @module goal
  state start
  outlook rainy
}
