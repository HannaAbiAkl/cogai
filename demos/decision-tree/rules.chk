rule r1 {
  @condition g1
  @action a1
}
golf g1 {
  @module goal
  state start
  outlook sunny
}
golf a1 {
  @module goal
  state humidity
}

rule r2 {
  @condition g2
  @action a2, a3
}
golf g2 {
  @module goal
  state humidity
  humidity ?num
  @lteq ?num, 75
}
golf a2 {
  @module goal
  state done
}
golf a3 {
  @module output
  value "play"
}

rule r3 {
  @condition g3
  @action a4, a5
}
golf g3 {
  @module goal
  state humidity
  humidity ?num
  @gt ?num, 75
}
golf a4 {
  @module goal
  state done
}
golf a5 {
  @module output
  value "don't play"
}

rule r4 {
  @condition g4
  @action a6, a7
}
